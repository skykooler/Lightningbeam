include						"Styles/Pcmanfm/pcmanfm.rc"

style "pcmanfm-window-no-menubar"
{
	engine "pixmap"
	{
		image
		{
			function        	= FLAT_BOX
			recolorable     	= TRUE
			#state			= PRELIGHT
			file			= "Apps/pcmanfm/pcmanfm-no-menubar.png"
			border			= { 0, 0, 28, 0}
			stretch         	= TRUE
		}
	}
}

#PCMANFM
class "FmMainWin"				style:highest "pcmanfm-window-no-menubar"
